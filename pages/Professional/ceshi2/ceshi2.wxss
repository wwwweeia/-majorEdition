/*table.wxss*/
.table {
  border: 0px solid darkgray;
  padding: 20rpx 20rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:75%;
    justify-content: center;
}
.td1 {
    width:25%;
    justify-content: center;
     text-align: center;
}
.bg-w{
  background: #0089dd;
  opacity: 0.6;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 75%;
  justify-content: center;
  background: #0089bb;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.th1 {
  width: 25%;
  justify-content: center;
  background: #0089bb;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}